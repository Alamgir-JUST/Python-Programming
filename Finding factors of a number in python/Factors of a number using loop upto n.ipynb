   "source": [
    "# Finding factors of a number in python"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "8c17c0c6",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a number: 10\n",
      "Factors of  10  are: \n",
      "1, 2, 5, 10, "
     ]
    }
   ],
   "source": [
    "number = int (input ('Enter a number: '))\n",
    "\n",
    "print ('Factors of ', number, ' are: ')\n",
    "\n",
    "for i in range(1, number + 1):\n",
    "    if number % i == 0:\n",
    "        print (i, end = ', ')"
   ]
  }
