   "source": [
    "# Prime Number Checking in Python using loop upto n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "2e1014ed",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a number: 97\n",
      "97  is a Prime Number.\n"
     ]
    }
   ],
   "source": [
    "num = int (input('Enter a number: '))\n",
    "\n",
    "count = 0\n",
    "\n",
    "for i in range(1, num + 1):\n",
    "    if num % i == 0:\n",
    "        count = count + 1\n",
    "\n",
    "if count == 2:\n",
    "    print (num, ' is a Prime Number.')\n",
    "else:\n",
    "    print (num, ' is not a Prime Number.')"
   ]
  },
