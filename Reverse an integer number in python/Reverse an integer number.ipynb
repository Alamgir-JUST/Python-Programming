    "# Python program to reverse an integer number"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "78b4a405",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a number: 123456\n",
      "Reverse of the given number is:  654321\n"
     ]
    }
   ],
   "source": [
    "num = int (input ('Enter a number: '))\n",
    "\n",
    "reverse = 0\n",
    "while num != 0:\n",
    "    r = num % 10\n",
    "    reverse = (reverse * 10) + r\n",
    "    num = int (num / 10)\n",
    "    \n",
    "print ('Reverse of the given number is: ', reverse)"
   ]
  }
